#class Vector2
|>
    #let x = 0 (<- R).
    #let y = 0 (<- R).

    distance() = sqrt(x * x + y * y) (void -> R).

    #process normalized() (void -> void)
    |>
        #let dist = distance() (<- R).

        #if dist = 0
        |>
            #print "Ops!".
        |>
        #otherwise
        |>
            x := x / dist.
            y := y / dist.
        |>
    |>
|>
